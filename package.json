{
  "name": "fqlx",
  "displayName": "fqlx",
  "description": "FQLX language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fqlx.helloWorld",
    "onLanguage:fqlx"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "fqlx.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "fqlx",
        "extensions": [
          ".fqlx"
        ]
      }
    ],
    "grammars": [
      {
        "language": "fqlx",
        "scopeName": "source.fqlx",
        "path": "./syntax/fqlx.tmGrammar.json"
      }
    ]
  }
}
